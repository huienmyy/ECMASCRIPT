{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": [
        "error", 
        "tab"
    ], 
    "linebreak-style": [
        "error", 
        "unix"
    ],
     "quotes": [
        "error", 
        "single"
    ], 
    "semi": [
        "error", 
        "always"
    ]
  }
}
